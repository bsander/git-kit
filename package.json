{
  "name": "git-kit",
  "version": "1.0.2",
  "description": "A collection of git workflow scripts",
  "author": "Sander Bouwhuis",
  "license": "MIT",
  "scripts": {
    "test": "[ -n \"$(which shellcheck)\" ] && shellcheck bin/* || echo 'Get shellcheck from https://github.com/koalaman/shellcheck'"
  },
  "keywords": [
    "git",
    "workflow"
  ],
  "bin": {
    "git-all-exec": "bin/git-all-exec",
    "git-current-branch": "bin/git-current-branch",
    "git-kill": "bin/git-kill",
    "git-merge-chain": "bin/git-merge-chain",
    "git-purge": "bin/git-purge",
    "git-pull-request": "bin/git-pull-request",
    "git-purge-remote": "bin/git-purge-remote",
    "git-remote-exists": "bin/git-remote-exists",
    "git-retire": "bin/git-retire",
    "git-select-remote": "bin/git-select-remote",
    "git-select-branch": "bin/git-select-branch",
    "git-sync": "bin/git-sync",
    "git-topic-finish": "bin/git-topic-finish",
    "git-topic-start": "bin/git-topic-start",
    "git-wipe": "bin/git-wipe"
  },
  "repository": "bsander/git-kit",
  "bugs": "https://github.com/bsander/git-kit/issues",
  "preferGlobal": true
}
